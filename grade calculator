#include<stdio.h>
int main()
{
	int marks;
	printf("enter the marks: ");
	scanf("%d",&marks);
	if(marks<40)
	{
		printf("grade F");
	}
	else if(marks>=40 && marks<55)
	{
		printf("grade D");
	}
	else if(marks>=55 && marks<70)
	{
		printf("grade C");
	}
	else if(marks>=70 && marks<85)
	{
		printf("grade B");
	}
	else if(marks>=85 && marks<=100)
	{
		printf("grade A");
	}
}
